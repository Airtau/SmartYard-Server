{
    "name": "syslog",
    "version": "1.0.0",
    "description": "Intercom Syslog service",
    "type": "commonjs",
    "scripts": {
        "dev": "concurrently -k \"npm run dev:*\"",
        "start": "concurrently -k \"npm run start:*\"",
        "start:beward_ds": "node beward.js --config=beward_ds",
        "start:qtech": "node qtech.js",
        "start:is": "node is.js",
        "start:akuvox": "node akuvox.js",
        "start:rubetek": "node rubetek.js",
        "start:sputnik": "node sputnik.js",
        "dev:beward": "nodemon beward.js --config=beward",
        "dev:beward_ds": "nodemon beward.js --config=beward_ds",
        "dev:qtech": "nodemon qtech.js",
        "dev:is": "nodemon is.js",
        "dev:akuvox": "nodemon akuvox.js",
        "dev:rubetek": "nodemon rubetek.js",
        "dev:sputnik": "nodemon sputnik.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.1.0",
        "concurrently": "^7.4.0",
        "syslog-server": "^1.0.1"
    },
    "devDependencies": {
        "nodemon": "^2.0.20"
    },
    "engines": {
        "npm": ">=8.0.0",
        "node": ">=18.0.0"
    }
}
